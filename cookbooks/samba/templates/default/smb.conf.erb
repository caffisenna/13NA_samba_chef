# See smb.conf.example for a more detailed config file or
# read the smb.conf manpage.
# Run 'testparm' to verify the config is correct after
# you modified it.

[global]
    #cups options = raw
    #printcap name = cups
    #printing = cups
    dos charset = CP932
    load printers = no
    netbios name = centos7
    os level = 20
    passdb backend = tdbsam
    security = user
    server string = SAMBA SERVER Version %v
    workgroup = WORKGROUP
    wins support = yes
    disable spoolss = yes
    max protocol = SMB3
    min protocol = SMB2

[homes]
    browseable = no
    comment = Home Directories
    writable = yes
    valid users = %S, %D%w%S
    available = no
    inherit acls = Yes

[13na-share]
    comment = ieei
    writeable = yes
    path = /home/share
    read only = no
    force create mode = 0666
    force directory mode = 0777

veto files=/.AppleDesktop/Network Trash Folder/.AppleDouble/TheVolumeSettingsFolder/TheFindByContentFolder/*.{*}/:eES_Store/.DS_Store/.Trashes/._.Trashes/._*
delete veto files = yes

